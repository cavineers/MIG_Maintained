{
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.server.launchMode": "Standard",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "bin/": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        "**/*~": true
    },
    "cSpell.words": [
        "AHRS",
        "CANPID",
        "Gson",
        "Odometry",
        "fout",
        "lvuser",
        "setpoint",
        "accl",
        "whitesmoke"
    ],
    "java.checkstyle.configuration": "${workspaceFolder}\\checkstyle.xml",
    "java.checkstyle.version": "8.41",
}